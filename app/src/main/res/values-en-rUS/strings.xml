    <string name="article_head" formatted="false">
        <![CDATA[
        <html>
            <head>
               <meta http-equiv="Content-Type" content="text/html;charset=gb2312">
                <style type="text/css">
                * {
                			padding: 0;
                			margin: 0;
                			}
                		html,
                			body { height: 100%;}
                			html {
                			font-size: 100%;
                			font-size: 1rem;
                			}
                			body {
                			width: 100%;
                			font-size: 12px;
                			font-family: SimHei, 黑体, 宋体;
                			color: #949393;
                			}
                			.block-title {
                			margin: 0 8px 8px 0;
                			padding: 8px 0;
                			border-bottom: 1px dashed #eee;
                			}
                			.block-title .title {
                			font-size: 1.125rem;
                			font-weight: normal;
                			border-left: 6px solid #ff851b;
                			padding: 2px 0 4px 8px;
                			text-shadow: 0 1px 4px rgba(0,0,0,0.1);
                			color: #333;
                			}
                			.block-title .name {
                			font-size: 14px;
                			border-left: 6px solid #ff851b;
                			text-align: right;
                			padding-right: 8px;
                			padding-top: 4px;
                			}
                			.block-title .name span {
                			color: #ff851b;
                			}
                			.icon {
                			margin-right: 5px;
                			font-size: .875rem;
                			color: #666;
                			}
                			</style>
            </head>
            <body>
            <div class="block-title">
            <h2 class="title">
        ]]>
